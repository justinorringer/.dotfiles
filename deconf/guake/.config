[general]
compat-delete='delete-sequence'
default-shell='/usr/bin/byobu'
display-n=0
display-tab-names=1
gtk-prefer-dark-theme=false
gtk-theme-name='Adwaita-dark'
history-size=1000
infinite-history=true
max-tab-name-length=100
mouse-display=false
new-tab-after=true
open-tab-cwd=true
prompt-on-quit=false
quick-open-command-line='gedit %(file_path)s'
restore-tabs-notify=true
restore-tabs-startup=true
save-tabs-when-changed=true
scroll-keystroke=true
start-at-login=true
start-fullscreen=true
tab-ontop=false
use-default-font=true
use-popup=true
use-scrollbar=false
use-trayicon=true
window-halignment=0
window-height=69
window-losefocus=false
window-refocus=false
window-tabbar=true
window-valignment=1
window-width=100

[keybindings/global]
show-hide='<Primary>grave'

[keybindings/local]
clipboard-copy='disabled'
clipboard-paste='disabled'
close-tab='<Primary><Shift>w'
close-terminal='disabled'
focus-terminal-down='disabled'
focus-terminal-left='disabled'
focus-terminal-right='disabled'
focus-terminal-up='disabled'
new-tab='<Primary><Shift>t'
new-tab-home='<Primary><Shift>h'
next-tab='<Primary>Right'
previous-tab='<Primary>Left'
rename-current-tab='<Primary><Shift>r'
search-on-web='disabled'
search-terminal='disabled'
split-tab-horizontal='disabled'
split-tab-vertical='disabled'
switch-tab-last='disabled'
switch-tab1='disabled'
switch-tab10='disabled'
switch-tab2='disabled'
switch-tab3='disabled'
switch-tab4='disabled'
switch-tab5='disabled'
switch-tab6='disabled'
switch-tab7='disabled'
switch-tab8='disabled'
switch-tab9='disabled'
toggle-hide-on-lose-focus='disabled'

[style]
cursor-shape=0

[style/background]
transparency=89

[style/font]
allow-bold=true
bold-is-bright=true
palette='#000000000000:#cccc66666666:#9999c1c1f1f1:#f0f0c6c67474:#8181a2a2bebe:#b2b29393bbbb:#8a8abebeb7b7:#fffffefefefe:#000000000000:#cccc66666666:#b5b5bdbd6868:#f0f0c5c57474:#8080a1a1bdbd:#b2b29494baba:#8a8abdbdb6b6:#fffffefefefe:#c5c5c8c8c6c6:#1d1d1f1f2121'
palette-name='Custom'
